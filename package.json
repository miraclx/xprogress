{
  "name": "xprogress",
  "version": "0.5.0",
  "description": "Dynamic, Flexible, extensible progressive CLI bar for the terminal built with NodeJS",
  "main": "dist/index.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "push": "git push github master && git push gitlab master",
    "build": "babel lib -sd dist",
    "version": "npm run build",
    "postversion": "npm run push"
  },
  "engines": {
    "node": ">=1"
  },
  "files": [
    "dist",
    "typings"
  ],
  "keywords": [
    "bar",
    "cli",
    "ascii",
    "progress",
    "progressbar",
    "stream",
    "speed",
    "bytes",
    "monitor",
    "percentage",
    "loader",
    "pulsate"
  ],
  "author": "Miraculous Owonubi <omiraculous@gmail.com>",
  "maintainers": [
    "Miraculous Owonubi <omiraculous@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miraclx/xprogress.git"
  },
  "bugs": {
    "url": "https://github.com/miraclx/xprogress/issues"
  },
  "license": "Apache-2.0",
  "babel": {
    "presets": [
      "airbnb",
      [
        "@babel/preset-env",
        {
          "corejs": true,
          "targets": {
            "node": 1
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "babel-eslint": "^10.0.1",
    "babel-preset-airbnb": "^3.2.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.5.0",
    "prettier": "^1.16.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "lodash": "^4.17.11",
    "pad-ratio": "^1.0.3",
    "progress-stream": "^2.0.0",
    "stringd": "^1.4.1",
    "stringd-colors": "^1.5.2",
    "xbytes": "^1.2.4"
  },
  "homepage": "https://github.com/miraclx/xprogress#readme",
  "directories": {
    "lib": "lib",
    "test": "test"
  }
}
